@misc{RefWorks:30,
	Abstract = {We propose a human emotion regarding social network, namely, the Affective Social Network. Our suggestion firstly builds a user{\^a}€{\texttrademark}s emotion profile in terms of the personality, mood and emotion by analysing the user{\^a}€{\texttrademark}s activities in social network. This subsequently builds an Emotional Relationship Matrix (ERM) which represents the depth of the emotional relationship based on the emotion profile. From our proposal, the more elaborate services based on the current user{\^a}€{\texttrademark}s emotional state can be provided to users. By considering emotional aspects in a social network, we can effectively answer which users or media contents will show the best results for inducing users{\^a}€{\texttrademark} emotional states. From the experiments, we verified that message containing emotional words and users{\^a}€{\texttrademark} relationship in a social network has significant correlations.},
	Author = {Hyun-Jun Kim and Seung-Bo Park and Geun-Sik Jo},
	Isbn = {1380-7501},
	Journal = {Multimedia Tools and Applications},
	Keywords = {Informatik},
	Pages = {1-20},
	Title = {Affective social network{\^a}€''happiness inducing social media platform}}

